        <sdk:Label Content="0000" FontSize="14" Height="19" HorizontalAlignment="Left" Margin="152,46,0,0" Name="labelNymphs" VerticalAlignment="Top" Width="46" />
        <sdk:Label Content="0000" FontSize="14" Height="19" HorizontalAlignment="Left" Margin="213,46,0,0" Name="labelAdults" VerticalAlignment="Top" Width="46" />
